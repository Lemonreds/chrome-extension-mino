{
    "name": "New Tab Page - Mino",
    "version": "0.0.4",
    "manifest_version": 2,
    "description": "a cool new tab extension for chrome.",
    "icons": {
      "16": "./static/icons/icon-16.png",
      "128": "./static/icons/icon-128.png"
    },
    "chrome_url_overrides": {
      "newtab": "tab.html"
    },
    "browser_action": {
        "default_icon": "./static/icons/icon-16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
      "chrome://favicon/",
      "tabs",   
      "downloads",
      "bookmarks",
      "storage",
      "http://*/*",
      "https://*/*"
    ], 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 
  }